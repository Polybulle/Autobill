decl sort idx

decl type f : (idx -> (idx -> idx))

decl type carrier : (idx -> (idx -> +))

decl type x0 : idx

decl type y0 : idx

pack foo_t (a : idx) (b : idx) =
  foo[(x : idx), (y : idx)]((carrier x y))
  with x = (f y a), y = (f x b)

decl val foor : (carrier x0 y0)

val fooz = foo[x0,y0](foor)

