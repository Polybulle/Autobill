decl sort idx

decl type carrier : (idx -> (idx -> -))

spec bar_aux_t =
  this.bar_aux[(x : idx), (y : idx)]().ret(): (carrier x y)
  with x = y

spec bar_t =
  this.bar[(x:idx)]().ret(): (carrier x x)

decl val baz_aux : bar_aux_t

val bazz = match this.bar[x1:idx]().ret(a) ->
  baz_aux.bar_aux[x1,x1]().ret(a)
